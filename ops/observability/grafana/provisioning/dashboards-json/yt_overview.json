{
  "uid": "yt_overview",
  "title": "YouTube Intel - Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Services Up",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(up{job=~\"yt_web|yt_worker\"})",
          "legendFormat": "up"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value"
      },
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "HTTP req/s (route,status)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (route,status) (rate(yt_http_requests_total[5m]))",
          "legendFormat": "{{route}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 16, "x": 8, "y": 0 }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Jobs (15m increase)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (type,status) (increase(yt_jobs_total[15m]))",
          "legendFormat": "{{type}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 4 }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Job duration p95 (ms)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le,type,status) (rate(yt_job_duration_ms_bucket[5m])))",
          "legendFormat": "{{type}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 10 }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Chat req/s (provider,status)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (provider,status) (rate(yt_chat_requests_total[5m]))",
          "legendFormat": "{{provider}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        },
        "overrides": []
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 20 }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] }
}

