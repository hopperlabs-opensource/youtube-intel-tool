{
  "name": "@yt/sdk",
  "version": "0.0.1",
  "description": "Type-safe SDK for the YouTube Intel Tool local API.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./routes": {
      "types": "./dist/routes.d.ts",
      "import": "./dist/routes.js",
      "default": "./dist/routes.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@yt/contracts": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  },
  "scripts": {
    "build": "tsup src/index.ts src/routes.ts --format esm --dts --clean --sourcemap",
    "clean": "rm -rf dist",
    "lint": "echo \"(sdk) no lint\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --import tsx --test test/contract.test.ts",
    "test:unit": "node --import tsx --test test/error-envelope.test.ts test/routes-parity.test.ts",
    "prepack": "pnpm run build"
  }
}
